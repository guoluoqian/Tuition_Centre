        <h1>Enquiry Form</h1>
        
        <div class="enquiry-container">
            <div class="form-section">
                <form>
                    <div class="form-group">
                        <label for="parent-name">Parent's Name *</label>
                        <input type="text" id="parent-name" required>
                    </div>
                    
                    <div class="form-group">
